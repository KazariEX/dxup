{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "nuxt",
      "type": "node",
      "request": "launch",
      "autoAttachChildProcesses": true,
      "runtimeExecutable": "${execPath}",
      "args": [
        "--folder-uri=${workspaceRoot}/packages/nuxt/playground"
      ],
      "outFiles": [
        "${workspaceRoot}/packages/nuxt/dist/**/*.cjs"
      ]
    },
    {
      "name": "unimport",
      "type": "node",
      "request": "launch",
      "autoAttachChildProcesses": true,
      "runtimeExecutable": "${execPath}",
      "args": [
        "--folder-uri=${workspaceRoot}/packages/unimport/playground"
      ],
      "outFiles": [
        "${workspaceRoot}/packages/unimport/dist/**/*.cjs"
      ]
    }
  ]
}
